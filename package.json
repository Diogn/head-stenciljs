    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@stencil/core": "^1.3.3",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.1.1",
    "@types/autoprefixer": "^9.6.1",
    "autoprefixer": "^9.7.3"
  },
  "license": "MIT",
  "dependencies": {
    "tailwindcss": "^1.1.4"
  }
}
